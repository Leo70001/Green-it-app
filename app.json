{
  "expo": {
    "name": "Green_it_app",
    "slug": "Green_it_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "greenitapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2A3836"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "mbizi.kamdem.greenapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2A3836"
      },
      "package": "mbizi.kamdem.greenapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "01c9a669-81c4-4b0e-b089-726164794421"
      }
    }
  }
}
